@page "/"

<ul>
    @foreach (string typeName in _allTypes.Keys)
    {
    <li>@typeName</li>
    }
</ul>

@functions {
    SortedDictionary<string, Type> _allTypes = new SortedDictionary<string, Type>();

    protected override void OnInit()
    {
        base.OnInit();
        foreach (Assembly assembly in AppDomain.CurrentDomain.GetAssemblies())
        {
            foreach (Type type in assembly.GetTypes())
            {
                if (type.IsPublic)
                {
                    _allTypes[type.FullName] = type;
                }
            }
        }
    }
}
